fn main() = {
    println("0 <= 10 is \(0 <= 10)").
    println("0 >= 10 is \(0 >= 10)").
    println("0 < 10 is \(0 < 10)").
    println("0 > 10 is \(0 > 10)").
    println("0 != 10 is \(0 != 10)").
    println("0 == 10 is \(0 == 10)").

    println("true = \(true)").
    println("false = \(false)").
    println("!true = \(!true)").
    println("!false = \(!false)").
}.